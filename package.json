{
  "name": "express-polymer-boilerplate",
  "version": "0.0.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "scripts": {
    "start": "node ./bin/www",
    "start-nodemon": "node ./nodemon",
    "react-start": "react-scripts start",
    "build": "react-scripts build",
